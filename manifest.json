{
  "name": "Buttgers",
  "version": "0.0.2",
  "description": "Replaces Rutgers with Buttgers",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_idle",
      "all_frames": true,
      "js": ["buttgers.js"]
    }
  ]
}